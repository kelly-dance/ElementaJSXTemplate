{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [],
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "jsxFactory": "createElement",
    "noImplicitUseStrict": true,
    "alwaysStrict": false
  }
}